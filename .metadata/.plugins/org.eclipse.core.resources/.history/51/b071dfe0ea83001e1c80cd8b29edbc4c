package com.skillstorm.assignment;

public class OrderOfOperations {
	
	public static void main(String[] args) {
		
		// Using modulus:
		System.out.println("Modulus questions:");
		int counter = 1;
		int  q = 5+7%6;
		int result = q;
		System.out.println("q " + counter + ": " + result);
		counter++;
	}

}
